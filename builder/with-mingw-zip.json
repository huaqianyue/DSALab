{
  "$schema": "http://json.schemastore.org/electron-builder",
  "productName": "DSALab",
  "directories": {
    "output": "release/"
  },
  "extraResources": [
    {
      "from": "./src/extraResources/",
      "to": "extraResources",
      "filter": [
        "**/*"
      ]
    }
  ],
  "files": [
    "dist/**/*"
  ],
  "artifactName": "${productName}-${version}-with-mingw.${ext}",
  "win": {
    "icon": "dist/assets/icons/favicon.ico",
    "fileAssociations": {
      "ext": ["cc", "cxx", "cpp", "h"],
      "description": "C++ source file"
    },
    "target": [
      "zip"
    ]
  }
}
