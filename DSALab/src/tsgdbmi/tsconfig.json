{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "declaration": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    // "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}